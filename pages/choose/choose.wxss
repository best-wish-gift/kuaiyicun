.btn1{
  width: 200rpx;
  height: 200rpx;
  margin-top: 200rpx;
  background-color: rgb(0, 128, 128);
  color: white;
  border-radius: 30px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 50rpx;
}

.btnImg {
  width: 50rpx;
  height: 50rpx;
}

.btn1::after {
  border: 0; 
}

